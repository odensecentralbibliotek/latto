;----------// Theme Information
  name        = latto
  description = Default theme for Ding2tal.
  core        = 7.x
  release     = 7.x-1.x
  screenshot  = screenshot.png

  ; Import stylesheets.
  stylesheets[all][] = css/latto.reset.css
  stylesheets[all][] = css/latto.bootstrap.css
  stylesheets[all][] = css/latto.styles.css

  ; Import javascripts.
  ; scripts[] = scripts/script.js

;----------// Regions
  regions[branding_top]    = Branding top
  regions[header]          = Header
  regions[content]         = Main Content
  regions[footer]          = Footer
  regions[branding_bottom] = Branding bottom

;----------// Polyfills
  settings[load_html5js] = 1

;----------// Plugins
  settings[load_equalize] = 1

;----------// Stylesheets

  settings[enable_exclude_css] = 0

  ; Stylesheets you may want to unset - includes all cores CSS files, except
  ; for overlay.
  unset_core[modules/aggregator/aggregator.css]     = 'aggregator.css'
  unset_core[modules/block/block.css]               = 'block.css'
  unset_core[modules/book/book.css]                 = 'book.css'
  unset_core[modules/color/color.css]               = 'color.css'
