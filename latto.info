  unset_core[modules/dashboard/dashboard.css]       = 'dashboard.css'
  unset_core[modules/dblog/dblog.css]               = 'dblog.css'
  unset_core[modules/field/theme/field.css]         = 'field.css'
  unset_core[modules/field_ui/field_ui.css]         = 'field_ui.css'
